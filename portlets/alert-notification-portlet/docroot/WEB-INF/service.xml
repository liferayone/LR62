<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.twelve.capital">
	<author>neha.kakkar</author>
	<namespace>aa</namespace>

	<entity name="adminalert" local-service="true" >

		<!-- PK fields -->

		<column name="alertId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="expirationDate" type="Date" />

		<!-- Other fields -->
		

		<column name="location" type="String" />
		<column name="resolution" type="String" />
		<column name="expectedResolutionTime" type="String" />
		<column name="resolutionStatus" type="String" />
		<column name="alertOpenTime" type="Date"/>
		<column name="alertResolvedtime" type="Date"/>
		
	</entity>
</service-builder>